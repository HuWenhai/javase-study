<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edgar.repository.TestTableMapper">
	<resultMap id="TestTableResultMap" type="TestTable">
		<result column="test_code" property="testCode" />
		<result column="dict_name" property="dictName" />
		<result column="parent_code" property="parentCode" />
		<result column="sorted" property="sorted" />
	</resultMap>

	<sql id="all_column">
		test_code, dict_name, parent_code, sorted
	</sql>

	<sql id="limit">
		<if test="limit != null">
			limit
			<if test="offset != null">
			#{offset},
			</if>
			#{limit}
		</if>
	</sql>

	<insert id="insert" parameterType="TestTable">
		insert into 
		test_table(test_code, dict_name, sorted) 
		values(#{testCode}, #{dictName}, #{sorted})
	</insert>

	<!-- START 写在START和END中间的代码不会被替换 -->

	<!-- END 写在START和END中间的代码不会被替换-->

</mapper>